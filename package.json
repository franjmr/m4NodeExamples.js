    "jasmine": "^3.5.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jsdom": "16.1.0",
    "jsdom-global": "3.0.2",
    "nyc": "^15.0.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
